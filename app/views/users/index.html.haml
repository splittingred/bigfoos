%ul.breadcrumb
  %li
    = link_to 'BigFoos', root_path
    %span.divider
  %li.active= t('users.title')

%p.pull-right= link_to t('.new'), new_user_path, class: 'btn btn-success'
%h1= t('users.title')


.table-responsive
  %table.table.table-striped
    %thead
      %tr
        %th
        %th Name
        %th Email
        %th W/L
        %th Signed up on
        %th
    %tbody
      = render users

= paginate users
